FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      k;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -2 0 0 0 0];

internalField   uniform 0;

boundaryField
{
    Inlet
    {
        type            fixedValue;
        value           uniform 2e-05;
    }

    Outlet
    {
        type            inletOutlet;
        inletValue      uniform 0;
        value           uniform 0;
    }

    FrontAndBack
    {
        type            empty;
    }

    SideWall
    {
        type            zeroGradient;
    }

    Wall
    {
        type            zeroGradient;
    }
}



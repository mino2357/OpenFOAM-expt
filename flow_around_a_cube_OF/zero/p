FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      p;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 -1 -2 0 0 0 0];

internalField   uniform 101325.0;

boundaryField
{
    Inlet
    {
        type            fixedValue;
	value		uniform 101325.0;
    }

    Outlet
    {
        type            zeroGradient;
	/*
        type            waveTransmissive;
        psi             thermo:psi;
        gamma           1.3;
        fieldInf        101325.0;
        lInf            0.3;
        value           $internalField;
	*/
    }

    SideWall
    {
        type            zeroGradient;
    }

    CubeWall
    {
        type            zeroGradient;
    }

    Ceiling
    {
        type            zeroGradient;
    }

    Floor
    {
        type            zeroGradient;
    }
}



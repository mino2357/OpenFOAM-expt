FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      p;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -2 0 0 0 0];

internalField   uniform 0.0;

boundaryField
{
    Inlet
    {
        // type            fixedValue;
	// value		uniform 0.0;
        type            zeroGradient;
    }

    Outlet
    {
        type            zeroGradient;
    }

    RightWall
    {
        type            zeroGradient;
    }

    LeftWall
    {
        type            zeroGradient;
    }

    CubeWall
    {
        type            zeroGradient;
    }

    Ceiling
    {
        type            zeroGradient;
    }

    Floor
    {
        type            zeroGradient;
    }
}



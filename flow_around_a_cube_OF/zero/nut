FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      nut;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -1 0 0 0 0];

internalField   uniform 0;

boundaryField
{
    Inlet
    {
        type            zeroGradient;
    }

    Outlet
    {
        type            zeroGradient;
    }

    SideWall
    {
        type            zeroGradient;
    }

    CubeWall
    {
        type            zeroGradient;
    }

    Ceiling
    {
        type            zeroGradient;
    }

    Floor
    {
        type            zeroGradient;
    }
}



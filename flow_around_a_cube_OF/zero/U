FoamFile
{
    version     2.0;
    format      ascii;
    class       volVectorField;
    object      U;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 -1 0 0 0 0];

internalField   uniform (1 0 0);

boundaryField
{
    Inlet
    {
        type            fixedValue;
        value           uniform (1 0 0);
    }

    Outlet
    {
        type            zeroGradient;
    }

    Ceiling
    {
        type            slip;
    }

    Floor
    {
        type            noSlip;
    }

    RightWall
    {
        type            slip;
    }

    LeftWall
    {
        type            slip;
    }

    CubeWall
    {
        type            noSlip;
    }
}


// ************************************************************************* //

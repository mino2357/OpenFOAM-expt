FoamFile
{
    version     2.0;
    format      ascii;
    class       volVectorField;
    object      U;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 -1 0 0 0 0];

internalField   uniform (10.0 0 0);

boundaryField
{
    Inlet
    {
        type            fixedValue;
        value           uniform (10 0 0);
    }

    Outlet
    {
        type            pressureInletOutletVelocity;
        inletValue      uniform (0 0 0);
        value           uniform (0 0 0);
    }

    Ceiling
    {
        type            slip;
    }

    Floor
    {
        type            noSlip;
    }

    SideWall
    {
        type            slip;
    }

    CubeWall
    {
        type            noSlip;
    }
}


// ************************************************************************* //

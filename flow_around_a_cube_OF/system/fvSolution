FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      fvSolution;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

solvers
{
    "(p|rho)"
    {
        solver          GAMG;
        smoother        DIC;
        //solver          PCG;
        //preconditioner  DIC;
        tolerance       1e-6;
        relTol          0.01;
    }

    "(p|rho)Final"
    {
        solver          GAMG;
        smoother        DIC;
        //solver          PCG;
        //preconditioner  DIC;
        tolerance       1e-9;
        relTol          0;
    }

    "(U|e|k|nuTilda|nut)"
    {
	solver          PBiCGStab;
        preconditioner  DILU;
        tolerance       1e-6;
        relTol          0.1;
    }

    "(U|e|k|nuTilda|nut)Final"
    {
	solver          PBiCGStab;
        preconditioner  DILU;
        tolerance       1e-9;
        relTol          0;
    }
}

PIMPLE
{
    momentumPredictor yes;
    nOuterCorrectors 2;
    nCorrectors     1;
    nNonOrthogonalCorrectors 0;

    pRefCell    0;
    pRefValue   0;
}

relaxationFactors
{
    equations
    {
        ".*"  1;
    }
}


